{
  "name": "@embedunit/plugins-sourcemap",
  "version": "0.5.0",
  "description": "Source map support plugin for embedunit",
  "license": "MIT",
  "author": "Vladislav Remin <vuliad@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/embedunit/embedunit.git",
    "directory": "packages/plugins/sourcemap"
  },
  "bugs": "https://github.com/embedunit/embedunit/issues",
  "homepage": "https://github.com/embedunit/embedunit#readme",
  "keywords": [
    "sourcemap",
    "source-map",
    "testing",
    "embedded",
    "javascript",
    "stack-trace"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": true,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "build:types": "rollup -c --configTypes",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@embedunit/core": "workspace:*",
    "source-map-js": "^1.2.1"
  }
}
