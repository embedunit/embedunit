{
  "name": "@embedunit/globals",
  "version": "0.4.0",
  "description": "Global installation utilities for embedunit",
  "license": "MIT",
  "author": "Vladislav Remin <vuliad@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/embedunit/embedunit.git",
    "directory": "packages/globals"
  },
  "bugs": "https://github.com/embedunit/embedunit/issues",
  "homepage": "https://github.com/embedunit/embedunit#readme",
  "keywords": [
    "globals",
    "testing",
    "embedded",
    "javascript",
    "describe",
    "it",
    "expect"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "build:types": "rollup -c --configTypes",
    "clean": "rm -rf dist",
    "test": "tsx test/run.ts"
  },
  "dependencies": {
    "@embedunit/core": "workspace:*",
    "@embedunit/assert": "workspace:*",
    "@embedunit/spy": "workspace:*"
  }
}
