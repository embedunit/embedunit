{
  "name": "@embedunit/spy",
  "version": "0.1.0",
  "description": "Spy and mock utilities for embedded JavaScript testing",
  "license": "MIT",
  "author": "Vladislav Remin <vuliad@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/embedunit/embedunit.git",
    "directory": "packages/spy"
  },
  "bugs": "https://github.com/embedunit/embedunit/issues",
  "homepage": "https://github.com/embedunit/embedunit#readme",
  "keywords": [
    "spy",
    "mock",
    "stub",
    "testing",
    "embedded",
    "javascript",
    "call-tracking"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "build:types": "rollup -c --configTypes",
    "clean": "rm -rf dist",
    "test": "tsx test/run.ts"
  },
  "dependencies": {
    "@embedunit/assert": "workspace:*"
  },
  "devDependencies": {
    "@embedunit/core": "workspace:*"
  }
}
