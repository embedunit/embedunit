{
  "name": "@embedunit/core",
  "version": "0.2.0",
  "description": "Tiny test runner core for embedded JavaScript runtimes",
  "license": "MIT",
  "author": "Vladislav Remin <vuliad@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/embedunit/embedunit.git",
    "directory": "packages/core"
  },
  "bugs": "https://github.com/embedunit/embedunit/issues",
  "homepage": "https://github.com/embedunit/embedunit#readme",
  "keywords": [
    "test",
    "testing",
    "test-runner",
    "embedded",
    "javascript",
    "runtime",
    "jest",
    "describe",
    "it",
    "hooks"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "build:types": "rollup -c --configTypes",
    "clean": "rm -rf dist",
    "test": "tsx test/run.ts"
  },
  "dependencies": {
    "source-map-js": "^1.2.1"
  },
  "devDependencies": {
    "@embedunit/assert": "workspace:*"
  }
}
