{
  "name": "embedunit",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r build",
    "build:types": "pnpm -r build:types",
    "build:bundles": "pnpm --filter @embedunit/bundles build",
    "test": "pnpm -r test",
    "test:e2e": "pnpm --filter @embedunit/e2e test:all",
    "test:e2e:node": "pnpm --filter @embedunit/e2e test:node",
    "test:e2e:deno": "pnpm --filter @embedunit/e2e test:deno",
    "test:e2e:quickjs": "pnpm --filter @embedunit/e2e test:quickjs",
    "test:e2e:browser": "pnpm --filter @embedunit/e2e test:browser",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "clean": "pnpm -r clean",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "glob": "^11.0.1",
    "globals": "^16.3.0",
    "lefthook": "^2.0.13",
    "rollup": "^3.29.5",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "tsx": "^4.20.4",
    "typescript": "^5.3.0"
  }
}
